 <!DOCTYPE html>
<html lang="es-ec">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
    <title>@ViewBag.Title | AI</title>
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet" media="screen" />
    <meta name="apple-mobile-web-app-title" content="AI">
    <meta name="application-name" content="AI">
    <meta name="msapplication-config" content="/Resources/Media/browserconfig.xml?v=gAazaK3O3l">
    <meta name="theme-color" content="#002264">
    <link href="https://fonts.googleapis.com/css?family=Asul|BioRhyme|Cormorant+Garamond|Dancing+Script|Lobster|Song+Myung|Tajawal|Tenali+Ramakrishna" rel="stylesheet">
    @*<link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">*@
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-114565605-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());
        gtag('config', 'UA-114565605-1');
    </script>

</head>
<body>
    <nav class="ui top attached inverted stackable menu "  id="main-menu">
        <a class="item" id="main-sidebar-trigger" >
            <i class="sidebar icon"></i>
            Menu
        </a>
        
         
    </nav>
    <aside id="main-sidebar" class="ui six wide vertical sidebar menu" >
        <br /><br /><br /><br />
                          <div class=" ui center aligned segment ">
                                      <h3 style="font-family: 'BioRhyme', cursive;">Menú Principal</h3>
                              <img src="~/Content/menu.jpg" width="250" height="150" />
                          </div>     
                              <div class="item  ">
                                  <br/>
                                  <div class="header">
                                      <h3>Menú</h3>
                                  </div>
                                  <div class="labeled compact menu">
                                      <a class="item" href="@Url.Action("Index", "Home")">
                                          <i class="fa fa-terminal  fa-2x" aria-hidden="true"></i>
                                          <span style="margin-right: 1.0em"></span>
                                          <span style="font-size: 16px;"><strong>Home</strong></span>
                                      </a>
                                      <a class="item" href="@Url.Action("TensorExample_Jd", "Home")">

                                          <i class="fa fa-terminal  fa-2x" aria-hidden="true"></i>
                                          <span style="margin-right: 1.0em"></span>
                                          <span style="font-size: 16px;"><strong>TensorFlow JS</strong></span>

                                      </a>
                                  </div>
                              </div>
                              
                        

    </aside>
    <main class="ui attached pusher">
        <div id="main-content">
             
              
                @RenderBody()
                 
             
        </div>
      
        <footer class="ui inverted vertical bottom attached   segment">
            
        </footer>
    </main>
    
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/semantics")
    <script>
        $('#main-sidebar')
            .sidebar('attach events', '#main-sidebar-trigger')
            .sidebar('setting', 'transition', 'overlay');
        $('.ui.dropdown').dropdown();
        $('.integrantes.modal')
                .modal('attach events', '.int.button', 'show')
            ;
        
    </script>
    @RenderSection("scripts", false)

</body>
</html>